{
  "$schema": "https://www.schemastore.org/package.json",
  "authors": ["Bernd Matzner <bernd.matzner@regard.io>"],
  "dependencies": {
    "@base-ui/react": "1.0.0",
    "@maptiler/leaflet-maptilersdk": "4.1.1",
    "@maptiler/sdk": "3.9.0",
    "@mdx-js/react": "3.1.1",
    "@regardio/js": "workspace:*",
    "@supabase/supabase-js": "2.89.0",
    "cmdk": "1.1.1",
    "cva": "beta",
    "embla-carousel": "8.6.0",
    "embla-carousel-react": "8.6.0",
    "fluid-tailwindcss": "1.0.5",
    "input-otp": "1.4.2",
    "intl-parse-accept-language": "1.0.0",
    "leaflet": "alpha",
    "lucide-react": "0.562.0",
    "markdown-to-jsx": "9.4.2",
    "next-themes": "0.4.6",
    "react": "19.2.3",
    "react-day-picker": "9.13.0",
    "react-dom": "19.2.3",
    "react-hook-form": "7.69.0",
    "react-resizable-panels": "4.0.11",
    "react-router": "7.11.0",
    "recharts": "3.6.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "vaul": "1.1.2",
    "zod": "4.2.1"
  },
  "description": "Regardio React UI components",
  "devDependencies": {
    "@regardio/dev": "workspace:*",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/leaflet": "1.9.21",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitest/ui": "4.0.16",
    "jsdom": "27.3.0",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vitest": "4.0.16"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./hooks/*": {
      "import": "./dist/hooks/*.js",
      "types": "./dist/hooks/*.d.ts"
    },
    "./tailwind.css": "./src/styles/tailwind.css",
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    }
  },
  "license": "MIT",
  "name": "@regardio/react",
  "private": false,
  "scripts": {
    "build": "exec-tsc --project tsconfig.build.json",
    "clean": "exec-clean .turbo dist",
    "dev": "exec-tsc --project tsconfig.build.json --watch",
    "fix": "exec-p fix:*",
    "fix:biome": "lint-biome check --write --unsafe .",
    "fix:md": "lint-md --fix \"**/*.md\" \"**/*.mdx\" \"!**/node_modules/**\" \"!**/dist/**\"",
    "lint": "exec-p lint:*",
    "lint:biome": "lint-biome check .",
    "lint:md": "lint-md \"**/*.md\" \"**/*.mdx\" \"!**/node_modules/**\" \"!**/dist/**\"",
    "prepare": "exec-husky",
    "report": "exec-s report:*",
    "report:unit": "vitest run --reporter html",
    "test": "exec-s test:*",
    "test:unit": "vitest run",
    "typecheck": "exec-tsc --noEmit"
  },
  "sideEffects": ["./src/styles/tailwind.css"],
  "type": "module"
}
