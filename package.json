{
  "$schema": "https://www.schemastore.org/package.json",
  "authors": ["Bernd Matzner <bernd.matzner@regard.io>"],
  "dependencies": {
    "@maptiler/leaflet-maptilersdk": "4.1.1",
    "@maptiler/sdk": "3.9.0",
    "@mdx-js/react": "3.1.1",
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-aspect-ratio": "1.1.8",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-hover-card": "1.1.15",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-menubar": "1.1.16",
    "@radix-ui/react-navigation-menu": "1.2.14",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-progress": "1.1.8",
    "@radix-ui/react-radio-group": "1.3.8",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toggle": "1.1.10",
    "@radix-ui/react-toggle-group": "1.1.11",
    "@radix-ui/react-tooltip": "1.2.8",
    "@radix-ui/themes": "3.2.1",
    "@regardio/js": "workspace:*",
    "@supabase/supabase-js": "2.86.0",
    "cmdk": "1.1.1",
    "cva": "beta",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "intl-parse-accept-language": "1.0.0",
    "leaflet": "alpha",
    "lucide-react": "0.555.0",
    "markdown-to-jsx": "9.1.2",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-day-picker": "9.11.2",
    "react-dom": "19.2.0",
    "react-hook-form": "7.66.1",
    "react-resizable-panels": "3.0.6",
    "react-router": "7.9.6",
    "recharts": "3.5.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "vaul": "1.1.2",
    "zod": "4.1.13"
  },
  "description": "Regardio React UI components",
  "devDependencies": {
    "@regardio/system": "workspace:*",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/leaflet": "1.9.21",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitest/ui": "4.0.14",
    "typescript": "5.9.3",
    "vite": "7.2.4",
    "vitest": "4.0.14"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./hooks/*": {
      "import": "./dist/hooks/*.js",
      "types": "./dist/hooks/*.d.ts"
    },
    "./shadcn/*": {
      "import": "./dist/shadcn/*.js",
      "types": "./dist/shadcn/*.d.ts"
    },
    "./tailwind.css": "./src/styles/tailwind.css",
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    }
  },
  "license": "MIT",
  "name": "@regardio/react",
  "private": false,
  "scripts": {
    "build": "exec-tsc --project tsconfig.build.json",
    "clean": "exec-clean .turbo dist",
    "dev": "exec-tsc --project tsconfig.build.json --watch",
    "lint": "exec-s lint:*",
    "lint:biome": "lint-biome check --write --unsafe .",
    "lint:md": "lint-md --fix \"**/*.md\" \"!**/node_modules/**\" \"!**/dist/**\"",
    "prepare": "exec-husky",
    "report": "exec-s report:*",
    "report:unit": "vitest run --reporter html",
    "test": "exec-s test:*",
    "test:unit": "vitest run",
    "typecheck": "exec-tsc --noEmit"
  },
  "sideEffects": ["./src/styles/tailwind.css"],
  "type": "module"
}
